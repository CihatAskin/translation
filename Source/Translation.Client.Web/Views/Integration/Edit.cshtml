@model IntegrationEditModel
    <a href="/Integration/Detail/@Model.IntegrationUid" data-translation="integration_detail_link">integration_detail_link</a>
<h1 data-translation="@Model.Title">@Model.Title</h1>

<form method="post">
    <partial name="Components/_Hidden" model="@Model.IntegrationInput" />

    <partial name="Components/_Input" model="@Model.IntegrationNameInput" />
    <partial name="Components/_LongInput" model="@Model.DescriptionInput" />

    <partial name="Components/_ErrorMessages" model="@Model.ErrorMessages" />

    <button type="submit" data-translation="update_integration_button">update_integration_button</button>
</form>
